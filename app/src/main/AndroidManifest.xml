<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.creditcardtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MonthSelectActivity"></activity>
        <activity android:name=".AirportMonthDetailActivity" />
        <activity android:name=".ItineraryList" />
        <activity android:name=".MonthList" />
        <activity android:name=".AirportFlights" />
        <activity android:name=".AirportListActivity" />
        <activity android:name=".EditLoyaltyActivity" />
        <activity android:name=".EditCardActivity" />
        <activity android:name=".AddLoyalty" />
        <activity android:name=".AddCard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>